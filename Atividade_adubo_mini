        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from scipy.optimize import linprog\n",
        "\n",
        "x = np.linspace(0, 30, 400)\n",
        "\n",
        "r1 = (3 - 2*x) / 3\n",
        "r2 = (6 - 3*x) / 3\n",
        "r3 = (4 - 4*x) / 2\n",
        "\n",
        "plt.figure(figsize=(8,6))\n",
        "\n",
        "plt.plot(x, r1, label='reta 1 2x1 + 3x2 >= 3')\n",
        "plt.plot(x, r2, label='reta 2 3x1 + 3x2 <= 6')\n",
        "plt.plot(x, r3, label='reta 3 4x1 + 2x2 >= 4')\n",
        "\n",
        "plt.xlim(0, 5)\n",
        "plt.ylim(0, 5)\n",
        "\n",
        "p1 = (0, 2)\n",
        "p2 = (3/4, 1,2)\n",
        "p3 = (1,5, 0)\n",
        "p4 = (2, 0)\n",
        "\n",
        "plt.xlabel(\"X1\")\n",
        "plt.ylabel(\"X2\")\n",
        "plt.title(\"Gráfico das Restrições\")\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "\n",
        "# VÉRTICES CORRETOS\n",
        "p1 = (0, 2)\n",
        "p2 = (3/4, 1/2)\n",
        "p3 = (1.5 , 0)\n",
        "p4 = (2, 0)\n",
        "\n",
        "vertices = [p1, p2, p3, p4]\n",
        "\n",
        "for px, py in vertices:\n",
        "    plt.scatter(px, py, color='red', s=80, zorder=5)\n",
        "    plt.text(px+0.5, py+0.5, f'({px:.2f}, {py:.2f})')\n",
        "\n",

        "!pip install pulp\n",
        "import pulp\n",
        "modelo = pulp.LpProblem(\"Exemplo\", pulp.LpMinimize)\n",
        "\n",
        "#x1, x2 >= 0\n",
        "x1 = pulp.LpVariable(\"X1\", lowBound=0)\n",
        "x2 = pulp.LpVariable(\"X2\", lowBound=0)\n",
        "\n",
        "# Função objetivo (minimizar)\n",
        "modelo += 10*x1 + 8*x2\n",
        "\n",
        "# Restrições\n",
        "modelo += 2*x1 + 3*x2 >= 3\n",
        "modelo += 3*x1 + 3*x2 >= 4\n",
        "modelo += 4*x1 + 2*x2 <= 6\n",
        "\n",
        "modelo.solve()\n",
        "\n",
        "print(\"Status:\", pulp.LpStatus[modelo.status])\n",
        "print(\"X1 =\", x1.varValue)\n",
        "print(\"X2 =\", x2.varValue)\n",
        "print(\"Valor ótimo =\", pulp.value(modelo.objective))\n"
      ],
